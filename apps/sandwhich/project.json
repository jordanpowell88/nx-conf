{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/sandwhich/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/sandwhich",
        "index": "apps/sandwhich/src/index.html",
        "baseHref": "/",
        "main": "apps/sandwhich/src/main.tsx",
        "polyfills": "apps/sandwhich/src/polyfills.ts",
        "tsConfig": "apps/sandwhich/tsconfig.app.json",
        "assets": [
          "apps/sandwhich/src/favicon.ico",
          "apps/sandwhich/src/assets"
        ],
        "styles": [
          "apps/sandwhich/src/styles.css"
        ],
        "scripts": [],
        "webpackConfig": "@nrwl/react/plugins/webpack"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/sandwhich/src/environments/environment.ts",
              "with": "apps/sandwhich/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/webpack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "sandwhich:build",
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "sandwhich:build:development"
        },
        "production": {
          "buildTarget": "sandwhich:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/sandwhich/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "component-test": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "apps/sandwhich/cypress.config.ts",
        "testingType": "component",
        "devServerTarget": "sandwhich:build",
        "skipServe": true
      }
    }
  },
  "tags": []
}
